<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2e ehttp://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd ">
    <display-name>Welcome to Tomcat</display-name>
    <description>Welcome to Tomcat</description>
    <!-- JSPC servlet mappings start
    -->
    <!-- JSPC servlet mappings end
    -->


    <servlet>
        <servlet-name>Service</servlet-name>
        <servlet-class>Service</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Service</servlet-name>
        <url-pattern>/Service</url-pattern>
    </servlet-mapping> 


    <servlet>
        <servlet-name>CommodityManage</servlet-name>
        <servlet-class>CommodityDispatcher</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>CommodityManage</servlet-name>
        <url-pattern>/CommodityManager</url-pattern>
    </servlet-mapping> 

    <servlet>
        <servlet-name>User</servlet-name>
        <servlet-class>UserManagementDispatcher</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>User</servlet-name>
        <url-pattern>/User</url-pattern>
    </servlet-mapping> 
    
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>Logout</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping> 

    <servlet>
        <servlet-name>sendMessage</servlet-name>
        <servlet-class>chatRoom.sendMessage</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>sendMessage</servlet-name>
        <url-pattern>/sendMessage</url-pattern>
    </servlet-mapping>
        <servlet>
        <servlet-name>checkMessage</servlet-name>
        <servlet-class>chatRoom.checkMessage</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>checkMessage</servlet-name>
        <url-pattern>/checkMessage</url-pattern>
    </servlet-mapping>
</web-app>
